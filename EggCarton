package eggcarton;

public class EggCarton
	{
	    private int KartNum;
	

	    public EggCarton(int day)
	    {
	    	KartNum = day;
	    }
	    
	    public int getCartonNum()
	    {
	        return KartNum;
	    }
	    
	    public int expireCalc()    
	    {
	        return KartNum + 14;
	    }
	}

